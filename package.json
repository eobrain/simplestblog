{
  "name": "simplestblog",
  "type": "module",
  "version": "0.0.1",
  "description": "Create a blog by editing markdown files",
  "main": "index.js",
  "scripts": {
    "prestart": "node index.js",
    "start": "cd _site && live-server",
    "demo": "rm -rf content && cp -r content-demo content && node index.js && cd _site && live-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix": "standard --fix --ignore layout/js/tex-svg-full.js --ignore layout/js/equation.js"
  },
  "repository": {
    "type": "git",
    "url": "github.com:eobrain/simplestblog.git"
  },
  "author": "Eamonn O'Brien-Strain",
  "license": "ISC",
  "dependencies": {
    "dompurify": "^3.1.2",
    "glob": "^10.3.12",
    "jsdom": "^24.0.0",
    "live-server": "^1.1.0",
    "marked": "^12.0.2",
    "marked-footnote": "^1.2.2",
    "mustache": "^4.2.0",
    "passprint": "^0.0.1"
  }
}
